      uint256 liquidityFee = (amount * LIQUIDITY_FEE) / 1000;
      uint256 recipientAmount = (amount - devFee - liquidityFee);
      _balances[recipient] += recipientAmount;
      _balances[DEV_ADDRESS] += devFee;
